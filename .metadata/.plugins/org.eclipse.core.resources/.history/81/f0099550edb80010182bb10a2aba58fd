package ejercicio2;

public class Ejercicio2 {

	public static void main(String[] args) {
		
	final int TOTAL_NUM = 11;
		
	int [][] bidimensional;
	bidimensional = new int [TOTAL_NUM][TOTAL_NUM];
	
		for (int y = 1; y < 11; y++) {
			for (int x = 1; x < 11; x++) {
				bidimensional[x][y] = x * y;
			}
		}
		
		for (int y = 1; y < 11; y++) {	
			for (int x = 1; x < 11; x++) {
				System.out.print(bidimensional[y][x] + "\t ");
			}
			System.out.println();
		}
	}

}
