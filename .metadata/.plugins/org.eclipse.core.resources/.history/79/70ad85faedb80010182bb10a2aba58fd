package ejercicio2;

public class Ejercicio2 {

	public static void main(String[] args) {
		
	final int TOTAL_NUM = 10;
		
	int [][] bidimensional;
	bidimensional = new int [TOTAL_NUM][TOTAL_NUM];
	
		for (int y = 1; y < 11; y++) {
			for (int x = 1; x < 11; x++) {
				for (int posiciony = 0; posiciony < 10; posiciony++) {
					for (int posicionx = 0; posicionx < 10; posicionx++) {
						bidimensional[posiciony][posicionx] = y * x;
						}
					}
			}
		}
		
		for (int posiciony = 0; posiciony < 10; posiciony++) {
			for (int posicionx = 0; posicionx < 10; posicionx++) {
				System.out.print(bidimensional[posiciony][posicionx] + "\t ");
			}
			System.out.println();
		}
	}

}
